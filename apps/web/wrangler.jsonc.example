{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ranksheet-web",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-08-15",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  }
  // REQUIRED: Add your Cloudflare account_id below
  // Get it from: https://dash.cloudflare.com/ -> Click your account -> Account ID
  // "account_id": "YOUR_CLOUDFLARE_ACCOUNT_ID"

  // OPTIONAL: Add project name if different from default
  // "project_name": "ranksheet-web"
}
